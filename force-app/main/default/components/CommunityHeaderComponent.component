                <div class="col-md-4 col-sm-6 col-xs-push-1" style="margin-left:20px;">
                    <ul class="functions" >
                        <li>
                            <a href="http://www.nxp.com/search" class="func-search">
                                <i class="fa fa-search">
                        
                     </i> {!$Label.Community_header_Search}
                            </a>
                            <form action="http://www.nxp.com/search" class="form-horizontal pull-right search slide-out">
                            <div class="form-group">
                                <div class="input-group">
                                    <input type="text" name="q" class="form-control input-lg search-box" placeholder="Search nxp.com" /><i class="fa fa-close"><!--_--></i>
